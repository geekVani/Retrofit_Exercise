<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_login"
    app:startDestination="@id/loginFragment3">

    <fragment
        android:id="@+id/loginFragment3"
        android:name="com.example.retrofitexercise.view.login.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment3_to_listFragment3"
            app:destination="@id/listFragment3" />
    </fragment>
    <fragment
        android:id="@+id/listFragment3"
        android:name="com.example.retrofitexercise.view.dashboard.ListFragment"
        android:label="ListFragment" >
        <action
            android:id="@+id/action_listFragment3_to_webFragment3"
            app:destination="@id/webFragment3" />
    </fragment>
    <fragment
        android:id="@+id/webFragment3"
        android:name="com.example.retrofitexercise.view.dashboard.WebFragment"
        android:label="WebFragment" />
</navigation>